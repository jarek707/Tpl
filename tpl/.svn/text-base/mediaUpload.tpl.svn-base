<center>
<object width="800" height="600" classid="clsid:8AD9C840-044E-11D1-B3E9-00805F499D93"
	codebase="http://java.sun.com/products/plugin/autodl/jinstall-1_5_0-windows-i586.cab#Version=1,5,0,0"> 
	<param name="code" value="com.jscape.ftpapplet.FtpApplet.class"> 
	<param name="archive" value="[%% urlRoot %%]/public/java/upload/sftpapplet.jar">
	<param name="scriptable" value="false">
	<comment>
		<embed
			type="application/x-java-applet;version=1.5"
			code="com.jscape.ftpapplet.FtpApplet.class"
			archive="[%% urlRoot %%]/public/java/upload/sftpapplet.jar"
			name="ftpapplet"
			width="800"
			height="600"
			scriptable="false"
			[%% IF isBroadcasterPack %%]
				createRemoteDir ="[%% broadcaster_id %%]/[%% broadcast_id %%]/[%% TIMESTAMP %%]"
				remoteDir="[%% broadcaster_id %%]/[%% broadcast_id %%]/[%% TIMESTAMP %%]"
				params="[%% urlRoot %%]/public/java/upload/paramPack.txt"
			[%% ELSE %%]
				params="[%% urlRoot %%]/public/java/upload/paramFree.txt"
			[%% ENDIF %%]
			pluginspage = "http://java.sun.com/products/plugin/index.html#download">
			<noembed></noembed>
		</embed>
	</comment>
</object>
</center>
