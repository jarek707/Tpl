<ul class="labelActive">
	<li>
		<label for="listLabel_main_events_chapCats">Label</label>
		<input type="text" id="listLabel_main_events_chapCats">
	</li>
	<li> 
		<label for="activateTarget_main_events_chapCats">&nbsp; &nbsp; Display Target Picture</label>
		<input type="checkbox" id="activateTarget_main_events_chapCats">
	</li>
	<li class="rightFlush">
		<label for="active_main_events_chapCats">Active</label>
		<input type="checkbox" id="active_main_events_chapCats">
	</li>

	<li class='lab1' style='position:relative; clear:both;'>
		<label class='lab1' for='comboIcon_[%% subAction %%]'>Combo Menu Icon</label>
	</li>
	<li>
		<input id='file_e_comboIcon_[%% subAction %%]' name='file_e_comboIcon_[%% subAction %%][]' type='file' />
		<input id='e_comboIcon_[%% subAction %%]' name='e_comboIcon_[%% subAction %%]' type='hidden' />
	</li>
	<div class='imgWrap'>
		<img id='img_l_comboIcon_[%% subAction %%]' class='chapCatImg' />
	</div>

	<li class='lab1' style='position:relative; clear:both;'>
		<label class='lab1' for='itemIcon_[%% subAction %%]'>Item Menu Icon</label>
	</li>
	<li>
		<input id='file_e_itemIcon_[%% subAction %%]' name='file_e_itemIcon_[%% subAction %%][]' type='file' />
		<input id='e_itemIcon_[%% subAction %%]' name='e_itemIcon_[%% subAction %%]' type='hidden' />
	</li>
	<div class='imgWrap'>
		<img id='img_l_itemIcon_[%% subAction %%]' class='chapCatImg' />
	</div>
</ul>
<ul>
	<input type='submit' class='textButton' value='Save' />
	<input type='button' class='textButton' value='Delete' onclick='ListEdit.Inst.doDelListData("[%% subAction %%]" );' />
</ul>
